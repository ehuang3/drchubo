#include <iostream>
#include<stdio.h>
#include<cmath>
#include<eigen3/Eigen/Dense>

using namespace Eigen;
using namespace std;

Vector3f divide (Vector3f goal){
Vector3f step;

float a = sqrt(goal(0)*goal(0)+goal(1)*goal(1)+goal(2)*goal(2));
step =goal/a;
step = 0.002*step;
cout << step <<endl<<endl<<a<<endl;

return step;
}

int main(){
Vector3f goal;
Vector3f current;
Vector3f step;

current << 0,0,0;
goal << 0,0.5,1;
step = divide(goal);

int n = 0;

while ((current(1) < goal(1))&&(current(2)<goal(2))){
  current = current + step;
  n+=1;
  cout <<current<< endl<<endl;
}
}
