    <include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <uri>model://drchubo</uri>
      <pose>0 0 1.25 0 0 0</pose>
      <plugin filename="libdrchubo_AnimationPush.so" name="push"/>
    </include>


  </world>
